<?xml version="1.0" encoding="UTF-8"?>
<document type="com.apple.InterfaceBuilder3.CocoaTouch.Storyboard"
          version="3.0"
          toolsVersion="20037"
          targetRuntime="iOS.CocoaTouch"
          propertyAccessControl="none"
          useAutolayout="YES"
          launchScreen="YES"
          colorMatched="YES"
          initialViewController="LAUNCH_SCREEN_VC">

    <device id="retina6_7" orientation="portrait" appearance="light"/>

    <dependencies>
        <plugIn identifier="com.apple.InterfaceBuilder.IBCocoaTouchPlugin" version="20020"/>
        <capability name="documents saved in the Xcode 8 format" minToolsVersion="8.0"/>
    </dependencies>

    <scenes>
        <!-- Launch Screen -->
        <scene sceneID="LAUNCH_SCENE">
            <objects>
                <viewController id="LAUNCH_SCREEN_VC" sceneMemberID="viewController">
                    <view key="view" userInteractionEnabled="NO" contentMode="scaleToFill" id="LAUNCH_ROOT_VIEW">
                        <rect key="frame" x="0" y="0" width="414" height="896"/>
                        <autoresizingMask key="autoresizingMask" widthSizable="YES" heightSizable="YES"/>

                        <subviews>
                            <label id="APP_TITLE"
                                   text="StrainSpotter"
                                   textAlignment="center"
                                   translatesAutoresizingMaskIntoConstraints="NO">
                                <fontDescription key="fontDescription" type="system" weight="bold" pointSize="32"/>
                                <color key="textColor" systemColor="labelColor"/>
                            </label>

                            <label id="APP_TAGLINE"
                                   text="Scan. Decode. Discover."
                                   textAlignment="center"
                                   translatesAutoresizingMaskIntoConstraints="NO">
                                <fontDescription key="fontDescription" type="system" pointSize="17"/>
                                <color key="textColor" systemColor="secondaryLabelColor"/>
                            </label>
                        </subviews>

                        <constraints>
                            <constraint firstItem="APP_TITLE" firstAttribute="centerX"
                                        secondItem="LAUNCH_ROOT_VIEW" secondAttribute="centerX" id="CENTER_X_TITLE"/>
                            <constraint firstItem="APP_TITLE" firstAttribute="centerY"
                                        secondItem="LAUNCH_ROOT_VIEW" secondAttribute="centerY" id="CENTER_Y_TITLE"/>

                            <constraint firstItem="APP_TAGLINE" firstAttribute="top"
                                        secondItem="APP_TITLE" secondAttribute="bottom" constant="12" id="TAGLINE_BELOW_TITLE"/>
                            <constraint firstItem="APP_TAGLINE" firstAttribute="centerX"
                                        secondItem="LAUNCH_ROOT_VIEW" secondAttribute="centerX" id="TAGLINE_CENTER_X"/>
                        </constraints>

                        <color key="backgroundColor" red="0.0196078431372549" green="0.050980392156862744" blue="0.03137254901960784" alpha="1" colorSpace="custom" customColorSpace="sRGB"/>

                    </view>
                </viewController>

                <placeholder placeholderIdentifier="IBFirstResponder"
                            id="FIRST_RESPONDER" sceneMemberID="firstResponder"/>
            </objects>
        </scene>
    </scenes>
</document>

